<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.finalhd.mapper.TUserMapper">
    <resultMap id="BaseResultMap" type="com.example.finalhd.entity.TUser" >
        <id column="id" property="id"  />
        <result column="email" property="email"  />
        <result column="nickname" property="nickname"  />
        <result column="password" property="password"  />
        <result column="type" property="type"  />
        <result column="username" property="username"  />
    </resultMap>

    <select id="loadUserByUsername" resultMap="BaseResultMap">
 select * from t_user where username=#{username}
</select>
    <insert id="regist">

        insert into t_user(username,password,email,nickname,type) value (#{username},#{password},#{email},#{nickname},1)
    </insert>
</mapper>
